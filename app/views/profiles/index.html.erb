<%= @profile.first_name %> <br>
<%= @profile.last_name %> <br>
<%= @profile.address %> <br>
<%= @profile.dob.strftime('%B/%d/%Y') %> <br>
<% if @profile.photo.key %>
  <%= cl_image_tag @profile.photo.key, height: 300, width: 400, crop: :fill %>
<% end %>
<%= @profile.document.filename %>




<%= simple_form_for(@profile) do |f| %>
  <%= f.input :photo, as: :file %>
  <%= f.input :document, as: :file %>
  <%= f.submit %>
<% end %>

<%= link_to "Go back", root_path, class: "btn btn-primary" %>
